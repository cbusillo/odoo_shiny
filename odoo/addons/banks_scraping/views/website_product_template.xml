<odoo>
    <template id="product_images" inherit_id="website_sale.product">
        <xpath expr="//div[contains(@class, 'product-images')]" position="after">
            <div class="product-images">
                <h3 class="mt16">Additional Images:</h3>
                <div t-foreach="product.product_images" t-as="image">
                    <img t-attf-src="data:image/*;base64,{{image.image_512}}" />
                </div>
            </div>
        </xpath>
    </template>
</odoo>
